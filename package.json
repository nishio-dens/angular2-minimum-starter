{
  "name": "angular2-minimum-starter",
  "version": "0.0.1",
  "description": "Angular2 Minimum starter",
  "author": "Shinsuke Nishio <nishio@densan-labs.net>",
  "license": "MIT",
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "scripts": {
    "watch": "webpack --watch --progress --profile --colors --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "build:prod": "webpack --progress --profile --colors --display-error-details --display-cached --optimize-occurence-order --optimize-minimize --optimize-dedupe",
    "server": "webpack-dev-server --inline --progress --profile --colors --display-error-details --display-cached --port 3000 --host 0.0.0.0",
    "start": "npm run server"
  },
  "dependencies": {
    "@angular/http": "2.0.0-rc.4",
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/forms": "^0.2.0",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "ie-shim": "^0.1.0",
    "core-js": "^2.4.0",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "~0.6.12",
    "http-server": "^0.9.0"
  },
  "devDependencies": {
    "css-loader": "^0.23.1",
    "exports-loader": "0.6.2",
    "expose-loader": "^0.7.1",
    "file-loader": "^0.8.4",
    "imports-loader": "^0.6.4",
    "json-loader": "^0.5.3",
    "es6-promise-loader": "^1.0.1",
    "raw-loader": "0.5.1",
    "url-loader": "^0.5.6",
    "reflect-metadata": "0.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tsconfig-lint": "^0.2.0",
    "tslint": "^3.7.1",
    "tslint-loader": "^2.1.3",
    "ts-helpers": "1.1.1",
    "ts-node": "^0.9.1",
    "typedoc": "^0.4.4",
    "typescript": "^2.0.0-beta",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0",
    "codelyzer": "~0.0.21"
  }
}
